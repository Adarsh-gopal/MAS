<odoo>
    
    <record id="view_sale_report_wizard" model="ir.ui.view">
        <field name="name">sale.register.wizard</field>
        <field name="model">sale.register.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form create="false" edit="false">
                <group>
                    <group>
                        <field name="start_date" string="Start Date" required='1'/>
                    </group>
                    <group>
                        <field name="end_date" string="End Date" required='1'/>
                    </group>
                </group>
            </form>
            <footer>
                <group>
                    <button class="btn btn-primary" type="object" name="retrieve_register" string="View Register"/>
                </group>
            </footer>
        </field>
    </record>
    
</odoo>