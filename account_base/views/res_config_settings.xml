<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.account_base</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="account.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <div id="account_yodlee" position="before">
                    <div class="col-12 col-lg-6 o_setting_box" id="activate_custom_seq_bankncash">
                        <div class="o_setting_left_pane">
                            <field name="enable_custom_sequence" widget="upgrade_boolean"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="enable_custom_sequence"/>
                            <div class="text-muted">
                                Set Custom Sequences
                            </div>
                        </div>
                    </div>
                </div>
            </field>
        </record>
    </data>
</odoo>